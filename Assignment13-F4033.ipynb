    "def fib(n):  \n",
    "     result = []\n",
    "     a, b = 0, 1\n",
    "     while a < n:\n",
    "         result.append(a)  \n",
    "         a, b = b, a+b\n",
    "     return result \n",
    "\n",
    "fib(56)"
   ]
